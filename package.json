{
  "name": "sincerify",
  "version": "1.0.0",
  "description": "Makes your code more transparent for debugger.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf .nyc_output out reports t*mp *.log *.t*mp *.lcov",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint --ext .js .",
    "test": "nyc mocha 'test/**/*.spec.js'"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/valango/sincerify.git"
  },
  "keywords": [
    "debug",
    "assert",
    "breakpoint",
    "debugger",
    "support",
    "diagnostics"
  ],
  "author": {
    "name": "Villem Alango",
    "email": "villem.alango@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/valango/sincerify/issues"
  },
  "homepage": "https://github.com/valango/sincerify#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "codecov": "^3.6.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.0"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "exclude": [],
    "include": [
      "[a-z]*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./reports"
  }
}
